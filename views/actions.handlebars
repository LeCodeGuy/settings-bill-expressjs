<div class="row">
    <h1>Actions</h1>
    <form action="/">
        <button  class="button-primary" type="submit" name="button">Back</button>
    </form>
    <table>
        <tr>
            <th>Type</th>
            <th>Price</th>
            <th>When</th>
        </tr>
        {{#each actions}}
        <tr>
            <td>{{type}}</td>
            <td>{{cost}}</td>
            <td>{{timestamp}}</td>
        </tr>
        {{/each}}
    </table>
</div>